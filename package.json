{
  "name": "kikoune",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --watch src-backend --ext ts,json --exec run-s build:backend run:backend",
    "build": "run-s build:*",
    "build:backend": "tsc -p tsconfig.node.json && tsc-alias -p tsconfig.node.json",
    "build:frontend": "vue-tsc && vite build",
    "run:backend": "node -r dotenv/config dist-backend/src-backend/index.js",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue src",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit"
  },
  "dependencies": {
    "@discord/embedded-app-sdk": "^1.0.2",
    "@fontsource/noto-sans-jp": "^5.0.18",
    "@hono/node-server": "^1.8.2",
    "@hono/zod-validator": "^0.2.0",
    "async-lock": "1.4.1",
    "consola": "^3.2.3",
    "discord-api-types": "^0.37.76",
    "expiry-map": "^2.0.0",
    "fast-xml-parser": "^4.3.6",
    "hono": "^4.1.3",
    "htmlparser2": "^9.1.0",
    "ioredis": "^5.3.2",
    "node-fetch": "^3.3.2",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.1.7",
    "tsconfig-paths": "^4.2.0",
    "uuid": "9.0.1",
    "vue": "^3.4.21",
    "vuedraggable": "^4.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/async-lock": "1.4.2",
    "@types/qs": "6.9.13",
    "@types/uuid": "9.0.8",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.3",
    "vite": "^5.2.2",
    "vue-tsc": "^1.8.27"
  }
}
